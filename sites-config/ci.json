{
  "artifactStructure": {
    "assets": [
      {
        "root": "client",
        "pattern": "**/*"
      }
    ],
    "features": "sites-config/features.json",
    "plugins": [
      {
        "pluginName": "Generator",
        "sourceFiles": [
          {
            "root": "dist/server",
            "pattern": "**/*{.ts,.mjs,.json}"
          }
        ],
        "event": "ON_PAGE_GENERATE",
        "functionName": "handle"
      }
    ]
  },
  "dependencies": {
    "installDepsCmd": "npm install",
    "requiredFiles": ["package.json", "package-lock.json", ".npmrc"]
  },
  "buildArtifacts": {
    "buildCmd": "npm run build"
  },
  "livePreview": {
    "serveSetupCmd": ":"
  }
}